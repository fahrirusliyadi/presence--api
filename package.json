{
  "name": "presence-api",
  "version": "1.0.0",
  "description": "Presence API with Express and TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "prepare": "husky",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@types/multer": "^1.4.12",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^2.0.22",
    "prettier": "3.5.3",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.29.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*": "prettier --ignore-unknown --write"
  }
}
